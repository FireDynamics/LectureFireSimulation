&HEAD CHID = 'reactionrate_arrhenius', TITLE = 'Finite_Rate' /

&MESH IJK = 40,40,40,  XB = -2,2, -2,2,  0,4 /

&TIME T_END = 10 /
&DUMP NFRAMES = 50 /

***********************************************
C3H8 + 5 O2 => 4 H2O + 3 CO2

&COMB INITIAL_UNMIXED_FRACTION = 0.0 /

&SPEC ID='NITROGEN', BACKGROUND = .TRUE./ 
&SPEC ID='PROPANE' /
&SPEC ID='OXYGEN' /
&SPEC ID='WATER VAPOR' /
&SPEC ID='CARBON DIOXIDE' /
&SPEC ID='CARBON MONOXIDE' /

&REAC ID = 'Fire'
      FUEL = 'PROPANE'
      A = 4e5
      E = 50
      SPEC_ID_NU = 'PROPANE', 'OXYGEN', 'WATER VAPOR', 'CARBON DIOXIDE'
      NU = -1, -5, 4, 3
      SPEC_ID_N_S = 'PROPANE', 'OXYGEN'
      N_S = 1,1 
      AUTO_IGNITION_TEMPERATURE = 50/

&INIT MASS_FRACTION(1) = 0.219,  SPEC_ID(1) = 'OXYGEN', 
      MASS_FRACTION(2) = 0.06,   SPEC_ID(2) = 'PROPANE'/

&INIT PART_ID='spheres', XYZ=0,0,2, N_PARTICLES=1 /
      
&PART ID='spheres', SURF_ID='HOT', STATIC=.TRUE., PROP_ID='ball' /

&SURF ID='HOT', TMP_FRONT=1500., RADIUS=0.5, GEOMETRY='SPHERICAL' /

&PROP ID='ball', SMOKEVIEW_ID='SPHERE', SMOKEVIEW_PARAMETERS(1)='D=0.01' /

***********************************************

&DEVC     /
&DEVC     /
&DEVC     /
&DEVC     /

&SLCF PBY =0.0 , QUANTITY = 'HRRPUV', CELL_CENTERED = .TRUE. /
&SLCF PBY =0.0 , QUANTITY = 'TEMPERATURE', CELL_CENTERED = .TRUE. /

&SLCF PBY = 0.0, QUANTITY = 'MASS FRACTION', SPEC_ID = 'OXYGEN' /
&SLCF PBY = 0.0, QUANTITY = 'MASS FRACTION', SPEC_ID = 'PROPANE' /

&TAIL /