      SPEC_ID(2) = 'CARBON DIOXIDE',  VOLUME_FRACTION(2) =    ,
      SPEC_ID(3) = 'WATER VAPOR',     VOLUME_FRACTION(3) =     /

&SPEC ID = 'NITROGEN', BACKGROUND = .TRUE. /

&REAC ID                        = 'Propane',
      FUEL                      = 'PROPANE',
      HEAT_OF_COMBUSTION        = 44715,
      SPEC_ID_NU                = 
